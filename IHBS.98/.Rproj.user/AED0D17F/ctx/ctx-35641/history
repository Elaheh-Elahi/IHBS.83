# Function to download files from a GitHub repository directory
download_files_from_repo <- function(owner, repo, directory, local_dir) {
gh::gh(repo = paste0(owner, "/", repo), cmd = paste("repo-contents", directory), dir = local_dir, clone = FALSE)
}
