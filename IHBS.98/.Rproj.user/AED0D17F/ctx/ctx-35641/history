library(gh)
owner <- "Elaheh-Elahi"
repo <- "IHBS_data_"
branch <- "main"
directory <- "data/98"
# Get list of files in the directory
files <- gh("/repos/:owner/:repo/contents/:path", owner = owner, repo = repo, path = directory, ref = branch)
View(files)
# Download files
for (file in files) {
download.file(file$download_url, basename(file$name))
}
