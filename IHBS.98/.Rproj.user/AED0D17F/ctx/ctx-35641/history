library(httr)
# Define function to download files from a GitHub repository
download_files_from_github <- function(repo_owner, repo_name, directory_path, target_directory) {
# Get the contents of the directory
response <- GET(paste0("https://api.github.com/repos/", repo_owner, "/", repo_name, "/contents/", directory_path))
if (response$status_code != 200) {
cat("Error:", content(response)$message, "\n")
return()
}
content <- content(response)
# Filter out the files from the directory content
files <- content[sapply(content, function(x) x$type == "file")]
# Loop through each file and download
for (file in files$name) {
download_url <- paste0("https://raw.githubusercontent.com/", repo_owner, "/", repo_name, "/main/", directory_path, "/", file)
download_file_path <- paste0(target_directory, "/", file)
cat("Downloading", download_url, "to", download_file_path, "\n")
download.file(download_url, destfile = download_file_path, mode = "wb")
}
}
# Usage example
repo_owner <- "Elaheh-Elahi"
repo_name <- "IHBS_data_"
directory_path <- "data/98"
target_directory <- "C:/Users/Dell/IHBS.98/data"
download_files_from_github(repo_owner, repo_name, directory_path, target_directory)
library(httr)
# Define function to download files from a GitHub repository
download_files_from_github <- function(repo_owner, repo_name, directory_path, target_directory) {
# Get the contents of the directory
response <- GET(paste0("https://api.github.com/repos/", repo_owner, "/", repo_name, "/contents/", directory_path))
if (response$status_code != 200) {
cat("Error:", content(response)$message, "\n")
return()
}
content <- content(response)
# Filter out the files from the directory content
files <- content[sapply(content, function(x) x$type == "file")]
# Loop through each file and download
for (file in files$name) {
download_url <- paste0("https://raw.githubusercontent.com/", repo_owner, "/", repo_name, "/main/", directory_path, "/", file)
download_file_path <- paste0(target_directory, "/", file)
cat("Downloading", download_url, "to", download_file_path, "\n")
download_result <- try(download.file(download_url, destfile = download_file_path, mode = "wb"), silent = TRUE)
if(class(download_result) == "try-error") {
cat("Error downloading:", download_url, "\n")
} else {
cat("Download successful.\n")
}
}
}
