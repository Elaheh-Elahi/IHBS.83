library(httr)
# Define function to download files from a GitHub repository
download_files_from_github <- function(repo_owner, repo_name, directory_path, target_directory) {
# Get the contents of the directory
response <- GET(paste0("https://api.github.com/repos/", repo_owner, "/", repo_name, "/contents/", directory_path))
content <- content(response)
# Filter out the files from the directory content
files <- content[sapply(content, function(x) x$type == "file")]
# Loop through each file and download
for (file in files$name) {
download_url <- paste0("https://raw.githubusercontent.com/", repo_owner, "/", repo_name, "/main/", directory_path, "/", file)
download.file(download_url, destfile = paste0(target_directory, "/", file), mode = "wb")
}
}
